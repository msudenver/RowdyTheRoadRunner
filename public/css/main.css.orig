/***************HighlightBox *********************/
/***************TRIANGLE COLOR RULE *********************/
.HighlightBox .triangle {
  left: 50%;
  bottom: 0;
  top: 105%;
  border-bottom: 3em solid #16A085;
  transform-origin: 25% 5%;
  -ms-transform-origin: 25% 5%;
  -webkit-transform-origin: 25% 5%;
  z-index: -1;
  /* Safari */

  -webkit-transform: rotate(180deg);
  /* Firefox */

  -moz-transform: rotate(180deg);
  /* IE */

  -ms-transform: rotate(180deg);
  /* Opera */

  -o-transform: rotate(180deg);
  /* Internet Explorer */

  /*accepts 0, 1, 2, or 3 which will 
      rotate the element 0, 90, 180 or 
      270 degrees respectively.*/
  filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=-);
}
.linkcolumn .HighlightBox .triangle {
  border-bottom: 3em solid #c0392b;
}
